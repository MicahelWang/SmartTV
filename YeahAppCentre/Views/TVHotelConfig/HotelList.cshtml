@model PagedViewList<string>
<div class="row" id="list">
    <table id="viewTable" class="table table-striped table-bordered table-hover dataTable">
        <thead>
            <tr role="row">
                <th class="center">
                </th>
                <th>酒店名称</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Source)
            {
                <tr>
                    <td class="center">
                        <a class="empty" style="cursor:pointer;" onclick="expandTable(this);">
                            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
                        </a>
                        <input type="hidden" class="key" value="@item" />
                    </td>
                    <td>
                        @Html.Action("GetHotelNameById", new { hotelId = item})
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
