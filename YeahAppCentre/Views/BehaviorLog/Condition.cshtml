@model YeahTVApi.DomainModel.SearchCriteria.LogCriteria

<div class="row">

    <div class="col-sm-12 tab-search">
        @using (Html.BeginForm("Index", "BehaviorLog", FormMethod.Post, new { novalidate = "novalidate" }))
        {
            <label> 日志内容</label>
            <span class="input-icon">
                @Html.EditorFor(model => model.LogInfo, new { htmlAttributes = new { @class = "" } })
            </span>
            <span class="input-icon">
                <button type="submit" class="btn btn-sm btn-success">查询</button>
            </span>
        }
        <div class="clear"></div>
    </div>
</div>
