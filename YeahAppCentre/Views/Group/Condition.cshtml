
@model YeahTVApi.DomainModel.SearchCriteria.GroupCriteria
<div class="row">

    <div class="col-sm-12 tab-search">
        @using (Html.BeginForm("Index", "Group", FormMethod.Post, new { novalidate = "novalidate" }))
        {
            <label> 集团名称</label>
            <span class="input-icon">
                <input id="GroupName" name="GroupName" value="" class="form-control" data-provide="typeahead" autocomplete="off" />
            </span>
            <span class="input-icon">
                <button type="submit" class="btn btn-sm btn-success">查询</button>
            </span>
        }
        <div class="clear"></div>
    </div>
</div>